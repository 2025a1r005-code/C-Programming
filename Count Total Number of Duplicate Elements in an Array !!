#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int arr[N];
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }

    int counted[N]; 
    for (int i = 0; i < N; i++) {
        counted[i] = 0;
    }

    int duplicate_count = 0;

    for (int i = 0; i < N; i++) {
        if (counted[i] == 1) continue; 

        int count = 1;
        for (int j = i + 1; j < N; j++) {
            if (arr[i] == arr[j]) {
                count++;
                counted[j] = 1; 
            }
        }

        if (count > 1) {
            duplicate_count++;
        }
    }

    printf("%d\n", duplicate_count);
    return 0;
}
