#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int countv=0,countc=0;
    char str[1001];
    scanf(" %[^\n]%*c",str);
    for(int i=0;str[i]!='\0';i++){
        if((str[i]>='a' && str[i]<='z') || (str[i]>='A' && str[i]<='Z')){
            
            if(str[i]=='a'|| str[i]=='e' || str[i]=='i' || str[i]=='o' || str[i]=='u' || str[i]=='A' || str[i]=='E' || str[i]=='I' || str[i]=='O' || str[i]=='U'){
                countv=countv+1;
            }
            else{
                countc=countc+1;
            }
        }
    }
    printf("%d %d",countv,countc);
    
    return 0;
}
