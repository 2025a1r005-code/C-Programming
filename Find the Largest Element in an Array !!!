#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main(){
    int N;
    scanf("%d",&N);
    int arr[N];
    for(int i=0;i<N;i++){
        scanf("%d",&arr[i]);
    }
    int max=arr[0];
    for(int j=0;j<N;j++){
        if(max<arr[j]){
            max=arr[j];
        }
    }
    printf("%d",max);
    return 0;
    
}
