#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdio.h>
#include <stdio.h>
#include <stdio.h>

int main() {
    int N, key;
    scanf("%d", &N);
    int arr[100]; 
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }
    scanf("%d", &key);
    int count = 0;
    for (int i = 0; i < N; i++) {
        if (arr[i] == key) {
            count++;
        }
    }
    printf("%d\n", count);

    return 0;
}
