#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N;
    scanf("%d",&N);
    int num,max;
    scanf("%d",&num);
    max=num;
    for(int i=1;i<N;i++){
        scanf("%d",&num);
        if(num>max){
        max=num;
        }    
    }
    printf("%d",max);
    return 0;
}
