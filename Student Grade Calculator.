#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int m1,m2,m3,m4,m5;
    scanf("%d%d%d%d%d",&m1,&m2,&m3,&m4,&m5);
    float per;
    per=(m1+m2+m3+m4+m5)/5.0;
    printf("Percentage = %.2f\n",per);
    if(per>=90){
        printf("Grade = A");
    }
    else if(per>=80 && per<90){
        printf("Grade = B");
    }
    else if(per>=70 && per<80){
        printf("Grade = C");
    }
    else if(per>=60 && per<70){
        printf("Grade = D");
    }
    else if(per>=40 && per<60){
        printf("Grade = E");
    }
    else{
        printf("Grade = F");
    }
    return 0;
}
