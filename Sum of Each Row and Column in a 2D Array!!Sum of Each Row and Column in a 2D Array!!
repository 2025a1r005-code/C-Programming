#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int R,C;
    scanf("%d%d",&R,&C);
    int arr[R][C];
    for(int i=0;i<R;i++){
        for(int j=0;j<C;j++){
            scanf("%d",&arr[i][j]);
        }
    }
    for(int i=0;i<R;i++){
        int sumr=0;
        for(int j=0;j<C;j++){
            sumr=sumr+arr[i][j];
        }
        printf("%d",sumr);
        printf("\n");
    }
    for(int j=0;j<C;j++){
        int sumc=0;
        for(int i=0;i<R;i++){
            sumc=sumc+arr[i][j];
        }
        printf("%d",sumc);
        printf("\n");
    }
    
    
    
    return 0;
}
